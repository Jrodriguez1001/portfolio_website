---
import Layout from "../layouts/Layout.astro";
import Navbar from "../components/Navbars/Navbar.astro";
import HomeIntro from "../components/homepage/HomeIntro";
import QuickLinks from "../components/common/quickLinks";
import WorkExperiences from "../components/homepage/WorkExperiences";
import LinkedInRecommendations from "../components/homepage/LinkedInRecommendations";
import SkillsSection from "../components/homepage/SkillsSection";
---

<Layout title="Home">
  <main class="mb-20">
    <Navbar />
    <HomeIntro client:load />
    <QuickLinks client:visible />
    <WorkExperiences client:visible />
    <SkillsSection client:visible />
    <LinkedInRecommendations client:visible />
  </main>
</Layout>
